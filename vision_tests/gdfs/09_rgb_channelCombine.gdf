data input_rgbx = image:768,512,RGBA:READ,inputs/face.jpg

data input_rgb = image:768,512,RGB2
node org.khronos.openvx.color_convert input_rgbx input_rgb

data channel_r = image:768,512,U008
node org.khronos.openvx.channel_extract input_rgb !CHANNEL_R channel_r

data channel_g = image:768,512,U008
node org.khronos.openvx.channel_extract input_rgb !CHANNEL_G channel_g

data channel_b = image:768,512,U008
node org.khronos.openvx.channel_extract input_rgb !CHANNEL_B channel_b

data output_channel_combine = image:768,512,RGB2:WRITE,openvx_test_results/09_rgb_channel_combine_768x512.rgb
node org.khronos.openvx.channel_combine channel_r channel_g channel_b null output_channel_combine