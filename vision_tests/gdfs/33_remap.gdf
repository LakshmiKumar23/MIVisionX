data input_iyuv = image:1280,720,IYUV:READ,inputs/stm_1280x720.yuv
data inter_luma = image:1280,720,U008
node org.khronos.openvx.channel_extract input_iyuv !CHANNEL_Y inter_luma

data input_remap_table = remap:1280,720,1280,720:READ,inputs/remap_1280x720_mirror.txt
data output_remap_nearest = image:1280,720,U008:WRITE,openvx_test_results/33_remap_nearest_1280x720.yuv
node org.khronos.openvx.remap inter_luma input_remap_table !NEAREST_NEIGHBOR output_remap_nearest

data output_remap_bilinear = image:1280,720,U008:WRITE,openvx_test_results/33_remap_bilinear_1280x720.yuv
node org.khronos.openvx.remap inter_luma input_remap_table !BILINEAR output_remap_bilinear
