node org.khronos.openvx.add inter_sobel_3x3_x inter_luma !SATURATE output_add_5

data output_add_6 = image:1280,720,S016
node org.khronos.openvx.add inter_sobel_3x3_x inter_sobel_3x3_y !WRAP output_add_6

data output_add_7 = image:1280,720,S016
