data input_iyuv = image:1280,720,IYUV:READ,inputs/stm_1280x720.yuv

data input_rgb = image:1280,720,RGB2
node org.khronos.openvx.color_convert input_iyuv input_rgb

data channel_r = image:1280,720,U008:WRITE,openvx_test_results/10_rgb_channel_r_extract_1280x720.rgb
node org.khronos.openvx.channel_extract input_rgb !CHANNEL_R channel_r

data channel_g = image:1280,720,U008:WRITE,openvx_test_results/10_rgb_channel_g_extract_1280x720.rgb
node org.khronos.openvx.channel_extract input_rgb !CHANNEL_G channel_g

data channel_b = image:1280,720,U008:WRITE,openvx_test_results/10_rgb_channel_b_extract_1280x720.rgb
node org.khronos.openvx.channel_extract input_rgb !CHANNEL_B channel_b